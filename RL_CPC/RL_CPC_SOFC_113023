x=np.array([np.linspace(0,1,100)])
xx=expand(x)
y=[]
plt.figure()
plt.title('G')
for i in B.Wcat[:]:
    y.append(np.dot(np.array(i),xx))
    
    plt.plot(x.T,y[-1].T)
    # plt.plot(B.catL[:,1],B.catL[:,0],linestyle='None',marker='.')
    # plt.ylim([-0.5, 1])

xxL=expand_linear(x)
y2=[]
plt.figure()
plt.title('M')
for i in B.Mwcat[:]:
    y2.append(np.dot(np.array(i),xxL))
    
    plt.plot(x.T,y2[-1].T)
 

y3=[]
plt.figure()
plt.title('M_p')
for i in B.Mpwcat[:]:
    y3.append(np.dot(np.array(i),xx))
    
    plt.plot(x.T,y3[-1].T)


